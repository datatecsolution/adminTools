	<queryString>
		<![CDATA[SELECT
	recibo_pago.no_recibo AS no_recibo, 
	recibo_pago.fecha AS fecha, 
	recibo_pago.codigo_cliente AS codigo_cliente, 
	recibo_pago.total_letras AS total_letras, 
	recibo_pago.total AS total, 
	recibo_pago.saldo_anterio AS saldo_anterio, 
	recibo_pago.saldo AS saldo, 
	recibo_pago.concepto AS concepto, 
	recibo_pago.usuario AS usuario, 
	cliente.nombre_cliente AS nombre_cliente, 
	cliente.rtn, 
	empleados.codigo_empleado, 
	empleados.nombre as nombre_cobrador, 
	empleados.apellido as apellido_cobrador, 
	empleados.telefono as telefono_cobrador
FROM
	recibo_pago
	INNER JOIN
	cuentas_facturas
	ON 
		recibo_pago.codigo_cliente = cuentas_facturas.codigo_cuenta
	INNER JOIN
	cliente
	ON 
		cuentas_facturas.codigo_cliente = cliente.codigo_cliente
	INNER JOIN
	empleados
	ON 
		cliente.id_vendedor = empleados.codigo_empleado
WHERE 
	 `recibo_pago`.`fecha` BETWEEN $P{fecha_min} AND $P{fecha_max} and empleados.codigo_empleado= $P{codigo_vendedor}]]>
	</queryString>
